{
  "manifest_version": 2,

  "name": "TabController",
  "description": "TabController is an extension for Google Chrome that helps you manage your tabs.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"},

  "browser_action": {
    "default_title": "Manage chrome tabs",
    "default_icon": "images/icon19.png",
    "default_popup": "popup.html"
  }


}
