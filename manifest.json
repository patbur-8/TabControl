{
  "manifest_version": 2,

  "name": "TabController",
  "description": "TabController is an extension for Google Chrome that helps you manage your tabs.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"},

  "browser_action": {
    "default_title": "Manage chrome tabs",
    "default_icon": "eight.png",
    "default_popup": "popup.html"
  }


}